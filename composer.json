{
    "name": "neoan3/installer",
    "homepage": "https://neoan3.rocks",
    "description": "official neoan3 installer",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "bin": [
        "bin/neoan3"
    ],
    "autoload": {
        "psr-4": {
            "Neoan\\Installer\\": "src/",
            "Neoan\\Installer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "sroehrl",
            "email": "neoan@neoan.us"
        },
        {
            "name": "mhochm",
            "email": "moses@neoan.us"
        }
    ],
    "require": {
        "ext-zip": "*",
        "neoan3-apps/template": "^1.0",
        "neoan3-apps/ops": "^2.0",
        "ext-readline": "*",
        "ext-json": "*",
        "composer/composer": "2.1.9",
        "neoan3-apps/db": "^0.3",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
